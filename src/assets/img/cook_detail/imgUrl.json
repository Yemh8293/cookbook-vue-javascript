      "process": [
        {
          "ptitle": "步骤1",
          "pcontent": "提前准备好所需原料",
          "pic": "img/cook_detail/pic_01.jpg"
        },
        {
          "ptitle": "步骤2",
          "pcontent": "混合牛奶和色拉油，搅拌均匀",
          "pic": "img/cook_detail/pic_02.jpg"
        },
        {
          "ptitle": "步骤3",
          "pcontent": "加入蛋黄",
          "pic": "img/cook_detail/pic_03.jpg"
        },
        {
          "ptitle": "步骤4",
          "pcontent": "彻底搅拌均匀",
          "pic": "img/cook_detail/pic_04.jpg"
        },
        {
          "ptitle": "步骤5",
          "pcontent": "一次性倒入过筛的粉类（低粉+黑穗醋栗粉）",
          "pic": "img/cook_detail/pic_05.jpg"
        },
        {
          "ptitle": "步骤6",
          "pcontent": "彻底搅拌均匀",
          "pic": "img/cook_detail/pic_06.jpg"
        },
        {
          "ptitle": "步骤7",
          "pcontent": "蛋白分两次加入细砂糖",
          "pic": "img/cook_detail/pic_07.jpg"
        },
        {
          "ptitle": "步骤8",
          "pcontent": "打至如图的软弯钩状",
          "pic": "img/cook_detail/pic_08.jpg"
        },
        {
          "ptitle": "步骤9",
          "pcontent": "取三分之一的蛋白霜与之前的蛋黄糊切拌均匀",
          "pic": "img/cook_detail/pic_09.jpg"
        },
        {
          "ptitle": "步骤10",
          "pcontent": "然后再倒入剩余的蛋白霜中",
          "pic": "img/cook_detail/pic_10.jpg"
        },
        {
          "ptitle": "步骤11",
          "pcontent": "彻底切拌均匀",
          "pic": "img/cook_detail/pic_11.jpg"
        },
        {
          "ptitle": "步骤12",
          "pcontent": "倒入铺有油纸的烤盘当中（我用的是学厨28*28的方盘）",
          "pic": "img/cook_detail/pic_12.jpg"
        },
        {
          "ptitle": "步骤13",
          "pcontent": "将烤盘送入预热好的烤箱，175度烘焙18分钟",
          "pic": "img/cook_detail/pic_13.jpg"
        },
        {
          "ptitle": "步骤14",
          "pcontent": "烤好的蛋糕胚立刻取出，撕去油纸降温",
          "pic": "img/cook_detail/pic_14.jpg"
        },
        {
          "ptitle": "步骤15",
          "pcontent": "将淡奶油+糖粉+黑穗醋栗粉称重准备好",
          "pic": "img/cook_detail/pic_15.jpg"
        },
        {
          "ptitle": "步骤16",
          "pcontent": "打至如图的裱花状态",
          "pic": "img/cook_detail/pic_16.jpg"
        },
        {
          "ptitle": "步骤17",
          "pcontent": "蛋糕片切出一点斜坡状后，抹上打发的淡奶油（如图)",
          "pic": "img/cook_detail/pic_17.jpg"
        },
        {
          "ptitle": "步骤18",
          "pcontent": "然后卷起，送入冰箱冷藏定型2小时",
          "pic": "img/cook_detail/pic_18.jpg"
        },
        {
          "ptitle": "步骤19",
          "pcontent": "重复步骤15，再次打发淡奶油，然后装入裱花袋中",
          "pic": "img/cook_detail/pic_19.jpg"
        },
        {
          "ptitle": "步骤20",
          "pcontent": "在蛋糕卷顶部挤出花纹，放上黑穗醋栗和食用金箔装饰，即可切片食用",
          "pic": "img/cook_detail/pic_20.jpg"
        },
        {
